
        <div class="margs">
            <h1><%= @questions.count %> total questions</h1>
            <hr>
            <% @questions.each do |question| %>
            <div class="question-list">
                Q: <%= link_to question.title, question_path(question) %>
                <p>
                    posted <%= time_ago_in_words(question.created_at) %> ago. <%= answer_count(question) %>.
            </div>

                     <% end %>
                </p>
        </div>
